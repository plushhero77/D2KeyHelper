<Application
    x:Class="D2KeyHelper.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:D2KeyHelper.src.WPFConverters"
    xmlns:local="clr-namespace:D2KeyHelper"
    StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <converters:ProcToFormatConverter x:Key="D2ProcConv" />
        <converters:KeyUpDownEventConverter x:Key="KeyUpDownConv" />
        <converters:HookToFormatConverter x:Key="HookConv" />
        <converters:StrToBrushConverter x:Key="StrToBrushConv" />
        <converters:EnumToStringConverter x:Key="EnumToStringConv" />
    </Application.Resources>
</Application>
